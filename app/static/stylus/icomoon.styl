@font-face
  font-family 'icomoon'
  src url('../fonts/icomoon.eot?6kkry2')
  src url('../fonts/icomoon.eot?#iefix6kkry2') format('embedded-opentype'),
 url('../fonts/icomoon.woff?6kkry2') format('woff'),
		url('../fonts/icomoon.ttf?6kkry2') format('truetype'),
		url('../fonts/icomoon.svg?6kkry2#icomoon') format('svg')
  font-weight normal
  font-style normal

[class^="icon-"], [class*=" icon-"]
  font-family 'icomoon'
  speak none
  font-style normal
  font-weight normal
  font-variant normal
  text-transform none
  line-height 1
  -webkit-font-smoothing antialiased
  -moz-osx-font-smoothing grayscale

.icon-bell
