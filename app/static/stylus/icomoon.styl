    content "\e600"

.icon-bubble
  &:before
    content "\e601"

.icon-cog
  &:before
    content "\e602"

.icon-heart
  &:before
    content "\e603"

.icon-checkmark-circle
  &:before
    content "\e604"

.icon-share
  &:before
    content "\e605"

.icon-star
  &:before
    content "\e606"

.icon-chat
  &:before
    content "\e607"

.icon-plus
  &:before
    content "\e608"

